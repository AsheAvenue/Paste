<div class="snippet">
  <h1><%= @snippet['title'] %></h1>
  <div class="sbody" id="content">
     <%= @snippet['formatted_body'] %>
  </div>
  <div class="sdate">
     Created on <%= @snippet['created_at'].strftime('%a., %b.%-d, %Y at %l:%M%p') %>
  </div>
  <br/>
  <a href="/">Create new paste!</a>
</div>
